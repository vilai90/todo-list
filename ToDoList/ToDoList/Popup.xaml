        Title="Popup" Height="300" Width="353.732">
    <Grid Margin="0,0,-8,0">
        <Label x:Name="WarningLabel" Content="Label" HorizontalAlignment="Left" Margin="25,83,0,0" VerticalAlignment="Top"/>
        <Button x:Name="YesBtn" Content="Yes" HorizontalAlignment="Left" Margin="45,167,0,0" VerticalAlignment="Top" Width="75" Click="YesBtn_Click"/>
        <Button x:Name="NoBtn" Content="No" HorizontalAlignment="Left" Margin="215,167,0,0" VerticalAlignment="Top" Width="75" Click="NoBtn_Click"/>

    </Grid>
</Window>
